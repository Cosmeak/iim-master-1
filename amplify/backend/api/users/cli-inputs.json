{
  "version": 1,
  "paths": {
    "/updateUser": {
      "name": "/updateUser",
      "lambdaFunction": "updateUserDynamo",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update"
        ]
      }
    },
    "/getUser": {
      "name": "/getUser",
      "lambdaFunction": "getUserData",
      "permissions": {
        "setting": "private",
        "auth": [
          "read"
        ]
      }
    }
  }
}